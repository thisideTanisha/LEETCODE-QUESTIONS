class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
       int l=0,h=letters.length-1;
char result=letters[0];
while(l<=h){
    int mid = (l+h)/2;
    if(letters[mid]>target){
        result = letters[mid];
        h=mid-1;
    }
    else{
       l=mid+1;
    }
}
return result;
}
}
