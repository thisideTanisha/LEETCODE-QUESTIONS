class Solution {
    public int search(int[] nums, int target) {
        int low=0, high=nums.length-1;
            while(low<=high){
                int mid=(low+high)/2;
                if(nums[mid]==target){//if mid is target
                    return mid;
                }
                //left sorted
                if(nums[low]<=nums[mid]){
                 if(nums[low]<=target&& target<=nums[mid]){
                    high=mid-1;//if target in left part 
                 }
                 else{
                    low=mid+1;//else search in right part
                 }
                 
                }
                else{//if right side is sorted
                    if(nums[mid]<=target&& target<=nums[high]){
                        low=mid+1;//if target in right part ,search in right part
                    }
                    else{
                        high=mid-1;// else search in left part
                    }
                    
                }
            }
            return -1;
    }
}
