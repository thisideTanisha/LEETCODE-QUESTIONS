import java.util.*;
class Solution {
    public int majorityElement(int[] nums) {
      Arrays.sort(nums);
      int count=0;
      int result=0;
      for(int i=0;i<nums.length-1;i++){
        if(nums.length==1){
            return nums[0];
        }
            if(nums[i]==nums[i+1]){
                count ++;

                if(count>=((int)nums.length/2)){
                result=nums[i];
                return result;
            }

            }
      } 
      return result; 
    }
}
