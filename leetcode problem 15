public static void threesum(int [] nums){
    int n=nums.length;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            for(int k=j+1;k<n;k++){
                if(nums[i]+nums[j]+nums[k]==0){
                   System.out.println(nums[i]+" "+nums[j]+ " " +nums[k]);
                }
            }
        }
    }
   
}
